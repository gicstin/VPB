  <Target Name="GeneratePluginVersion" BeforeTargets="BeforeCompile">
    <PropertyGroup>
      <PluginVersionFile>$(MSBuildProjectDirectory)\plugin_version.txt</PluginVersionFile>
      <PluginVersionGeneratedFile>$(IntermediateOutputPath)PluginVersion.g.cs</PluginVersionGeneratedFile>
    </PropertyGroup>

    <PropertyGroup>
      <PluginBuildDayOfYear>$([System.String]::Format('{0:000}', $([System.DateTime]::Now.DayOfYear)))</PluginBuildDayOfYear>
      <PluginBuildTime>$([System.DateTime]::Now.ToString('HHmm'))</PluginBuildTime>
      <PluginBuildVersion>$(PluginBuildDayOfYear).$(PluginBuildTime)</PluginBuildVersion>
    </PropertyGroup>

    <ReadLinesFromFile File="$(PluginVersionFile)">
      <Output TaskParameter="Lines" ItemName="_PluginVersionLines" />
    </ReadLinesFromFile>

    <PropertyGroup>
      <PluginVersion>$([System.Text.RegularExpressions.Regex]::Replace('%(_PluginVersionLines.Identity)', '\\s+', ''))</PluginVersion>
    </PropertyGroup>

    <MakeDir Directories="$(IntermediateOutputPath)" />

    <ItemGroup>
      <_PluginVersionSourceLines Include="namespace VPB {" />
      <_PluginVersionSourceLines Include="    internal static class PluginVersionInfo {" />
      <_PluginVersionSourceLines Include="        public const string Version = &quot;$(PluginVersion)&quot;%3B" />
      <_PluginVersionSourceLines Include="        public const string BuildVersion = &quot;$(PluginBuildVersion)&quot;%3B" />
      <_PluginVersionSourceLines Include="    }" />
      <_PluginVersionSourceLines Include="}" />
    </ItemGroup>

    <WriteLinesToFile
      File="$(PluginVersionGeneratedFile)"
      Overwrite="true"
      Lines="@(_PluginVersionSourceLines)" />
  </Target>

